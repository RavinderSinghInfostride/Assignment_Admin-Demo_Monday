<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Cases" verbose="1" >
    <listeners >
        <listener class-name="Report.Listener"/>
    </listeners>
    <test name="Verify user is logged in">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId1" value="TCTC-1"/>
        <classes>
            <class name="Test.LoginTest"/>
        </classes>
    </test>

    <test name="Verify user able to add email account">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId2" value="TCTC-2"/>
        <classes>
            <class name="Test.AddEmailAccount"/>
        </classes>
    </test>

    <test name="Verify user able to edit store name">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId2" value="TCTC-2"/>
        <classes>
            <class name="Test.EditStoreName"/>
        </classes>
    </test>
</suite>