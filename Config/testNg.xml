<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Cases" verbose="1" >
    <listeners >
        <listener class-name="Report.Listener"/>
    </listeners>
    <test name="Verify user is logged in">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId1" value="TCTC-1"/>
        <classes>
            <class name="Test.LoginTest"/>
        </classes>
    </test>

    <test name="Edit customer">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId2" value="TCTC-2"/>
        <classes>
            <class name="Test.EditCustomerName"/>
        </classes>
    </test>

    <test name="Edit vendors details">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="testRunId3" value="TCTC-3"/>
        <classes>
            <class name="Test.SearchCustomer"/>
        </classes>
    </test>
</suite>